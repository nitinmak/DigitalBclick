<?xml version='1.0' encoding='utf-8'?>
<widget id="card.digitalcard.com" version="1.2.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">

    <icon src="icon/icon.png" />
    <name>digitalcard</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
   <access origin="http://bclickdigital.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        
        
        <allow-intent href="market:*" />
        <uses-permission android-name="android.permission.READ_CONTACTS" android-targetSdkVersion="28" />
        <uses-permission android-name="android.permission.WRITE_CONTACTS" android-targetSdkVersion="28" />
        <icon background="icon/icon.png" foreground="icon/icon.png" />
    </platform>
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="android-maxSdkVersion" value="28" />
    <platform name="ios">
        <icon height="180" src="icon/icon-60@3x.png" width="180" />
        <icon height="60" src="icon/icon-60.png" width="60" />
        <icon height="120" src="icon/icon-60@2x.png" width="120" />
        <icon height="76" src="icon/icon-76.png" width="76" />
        <icon height="152" src="icon/icon-76@2x.png" width="152" />
        <icon height="80" src="icon/icon-40@2x.png" width="80" />
        <icon height="57" src="icon/icon.png" width="57" />
        <icon height="114" src="icon/icon@2x.png" width="114" />
        <icon height="72" src="icon/icon-72.png" width="72" />
        <icon height="144" src="icon/icon-72@2x.png" width="144" />
        <icon height="58" src="icon/icon-small@2x.png" width="58" />
        <icon height="100" src="icon/icon-50@2x.png" width="100" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardShrinksView" value="true" />
    <plugin name="cordova-plugin-android-permissions" spec="1.0.0" />
    <plugin name="cordova-plugin-console" spec="1.0.7" />
    <plugin name="cordova-plugin-contacts" spec="1.0.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="2.1.3" />
    <plugin name="cordova-plugin-statusbar" spec="1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.1.0" />
    <gap:plugin name="cordova-plugin-whitelist" spec="1.1.0" />
    <preference name="fullscreen" value="false" />
</widget>
