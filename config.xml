<?xml version='1.0' encoding='utf-8'?>
<widget id="card.digitalcard.com" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
        <icon src="icon/icon.png" platform="ios" width="57" height="57" density="mdpi" />
            <icon src="icon/icon.png" />


    <name>digitalcard</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />

    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
  
  <icon background="icon/icon.png" foreground="icon/icon.png" />
  
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>

    <!-- <preference name="android-minSdkVersion" value="16" />  -->
        <!-- <preference name="android-targetSdkVersion" value="26" /> -->
         <preference name="android-targetSdkVersion" value="21" />
    <preference name="android-maxSdkVersion" value="21" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
     <plugin name="cordova-plugin-ionic-keyboard" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardShrinksView" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-contacts" spec="1.0.0" />

    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <preference name="fullscreen" value="false" />
    

</widget>
