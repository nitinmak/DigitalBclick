<?xml version='1.0' encoding='utf-8'?>
<widget id="card.digitalcard.com" version="1.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
        <icon src="icon/icon.png" platform="ios" width="57" height="57" density="mdpi" />
            <icon src="icon/icon.png" />


    <name>digitalcard</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
  
        <allow-intent href="market:*" />
        <uses-permission android-targetSdkVersion="28" android-name="android.permission.READ_CONTACTS" />
        <uses-permission android-targetSdkVersion="28" android-name="android.permission.WRITE_CONTACTS" />

        <icon background="icon/icon.png" foreground="icon/icon.png" />
  
    </platform>
        <!-- <icon src="icon/icon.png" platform="ios" width="1024" height="1024" /> -->

        <preference name="cordova-ios" version="4.5.4" />
         <preference name="phonegap-version" value="cli-8.0.0" />

    <!-- <preference name="android-minSdkVersion" value="16" />  -->
        <preference name="android-targetSdkVersion" value="28" />
         <!-- <preference name="android-targetSdkVersion" value="21" /> -->
    <preference name="android-maxSdkVersion" value="28" />
          
    <platform name="ios">
     	<icon height="57"  platform="ios"  src="icon.png"           width="57" />
    <icon height="114" platform="ios"  src="icon.png"        width="114" />
    <icon height="40"  platform="ios"  src="icon.png"        width="40" />
    <icon height="80"  platform="ios"  src="icon.png"     width="80" />
    <icon height="50"  platform="ios"  src="icon.png"        width="50" />
    <icon height="100" platform="ios"  src="icon.png"     width="100" />
    <icon height="60"  platform="ios"  src="icon.png"        width="60" />
    <icon height="120" platform="ios"  src="icon.png"     width="120" />
    <icon height="180" platform="ios"  src="icon.png"     width="180" />
    <icon height="72"  platform="ios"  src="icon.png"        width="72" />
    <icon height="144" platform="ios"  src="icon.png"     width="144" />
    <icon height="76"  platform="ios"  src="icon.png"        width="76" />
    <icon height="152" platform="ios"  src="icon.png"     width="152" />
    <icon height="29"  platform="ios"  src="icon.png"     width="29" />
    <icon height="58"  platform="ios"  src="icon.png"  width="58" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <plugin name="cordova-plugin-android-permissions" spec="1.0.0" />
<plugin name="cordova-plugin-console" spec="1.0.7" />
<plugin name="cordova-plugin-contacts" spec="1.0.0" />
<plugin name="cordova-plugin-ionic-keyboard" spec="2.1.3" />
<plugin name="cordova-plugin-statusbar" spec="1.0.1" />
<plugin name="cordova-plugin-whitelist" spec="1.2.2" />
     <!-- <plugin name="cordova-plugin-ionic-keyboard" /> -->
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardShrinksView" value="true" />
    <!-- <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-contacts" spec="1.0.0" />
       <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />


     --><plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="https://github.com/apache/cordova-plugin-whitelist.git" />
    <preference name="fullscreen" value="false" />
    

</widget>
